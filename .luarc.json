{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "lua/?.lua",
      "lua/?/init.lua"
    ],
    "pathStrict": true
  },
  "workspace": {
    "checkThirdParty": "Disable",
    "library": [
      "$VIMRUNTIME",
      "${env:XDG_DATA_HOME}/nvim/lazy/plenary.nvim"
    ],
    "ignoreDir": []
  },
  "diagnostics": {
    "unusedLocalExclude": [
      "Logger",
      "_*"
    ]
  }
}
